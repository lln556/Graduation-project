<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<ImageView
		android:id="@+id/personalInfo_ImageView_first"
		android:layout_width="373dp"
		android:layout_height="123dp"
		android:background="#ffffff"
		android:drawable="@drawable/rectangle"
		android:paddingLeft="10dp"
		android:paddingRight="10dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.36" />

	<ImageView
		android:id="@+id/headImage"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:src="@drawable/ic_user_head"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintBottom_toTopOf="@id/userName"
		/>
	<TextView
		android:id="@+id/userName"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintTop_toBottomOf="@id/headImage"
		android:text="用户名"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		android:textSize="20sp"
		app:layout_constraintBottom_toTopOf="@id/personalInfo_button_sixth"
		/>
		

	<Button
		android:id="@+id/personalInfo_button_second"
		android:layout_width="85dp"
		android:layout_height="50dp"
		android:background="@android:color/transparent"
		android:drawableTop="@drawable/wallet"
		android:gravity="center"
		android:paddingLeft="10dp"
		android:paddingRight="10dp"
		android:text="待支付"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.067"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.433" />

	<Button
		android:id="@+id/personalInfo_button_third"
		android:layout_width="85dp"
		android:layout_height="50dp"
		android:background="@android:color/transparent"
		android:gravity="center"
		android:paddingLeft="10dp"
		android:paddingRight="10dp"
		android:text="待收货"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.352"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.433" />

	<Button
		android:id="@+id/personalInfo_button_fourth"
		android:layout_width="85dp"
		android:layout_height="50dp"
		android:background="@android:color/transparent"
		android:gravity="center"
		android:paddingLeft="10dp"
		android:paddingRight="10dp"
		android:text="评价"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.637"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.433" />
	<Button
		android:id="@+id/personalInfo_button_fifth"
		android:layout_width="85dp"
		android:layout_height="50dp"
		android:background="@android:color/transparent"
		android:gravity="center"
		android:paddingLeft="10dp"
		android:paddingRight="10dp"
		android:text="售后/退款"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.923"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.433" />


	<Button
		android:id="@+id/personalInfo_button_sixth"
		android:layout_width="373dp"
		android:layout_height="33dp"
		android:background="@android:color/transparent"
		android:gravity="center_vertical"
		android:paddingLeft="25dp"
		android:paddingRight="10dp"
		android:text="我的订单"
		android:textSize="19dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.313" />

	<TextView
		android:id="@+id/personalInfo_textView_first"
		android:layout_width="130dp"
		android:layout_height="29dp"
		android:drawableRight="@drawable/jiantou"
		android:gravity="center"
		android:text="查看全部订单"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintHorizontal_bias="0.932"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.317" />

    <ImageView
        android:id="@+id/personalInfo_ImageView_second"
        android:layout_width="379dp"
        android:layout_height="165dp"
        android:background="#ffffff"
        android:drawable="@drawable/rectangle"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />
    <Button
        android:id="@+id/personalInfo_button_seventh"
        android:layout_width="373dp"
        android:layout_height="33dp"
        android:background="@android:color/transparent"
        android:gravity="center_vertical"
        android:paddingLeft="25dp"
        android:paddingRight="10dp"
        android:text="我的收货地址"
        android:textSize="19dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.600" />

    <TextView
        android:id="@+id/personalInfo_textView_second"
        android:layout_width="130dp"
        android:layout_height="29dp"
        android:drawableRight="@drawable/jiantou"
        android:gravity="center"
        android:text="设置收货地址"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.932"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.604" />
</androidx.constraintlayout.widget.ConstraintLayout>
